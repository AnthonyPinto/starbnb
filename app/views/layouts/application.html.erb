<!DOCTYPE html>
<html>
<head>
<%= favicon_link_tag 'favicon.ico' %>



  
  <title>Starbnb</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  
  <% if logged_in? %>
    <script type="text/javascript">
    window.CURRENT_USER_ID = <%= current_user.id %>
    </script>
  <% end %>
  <link <link href='http://fonts.googleapis.com/css?family=Lato:100' rel='stylesheet' type='text/css'>
</head>
<script type="text/javascript"
  src="//api.filepicker.io/v1/filepicker.js">
</script>
<body>
<div class="body-wrapper container-fluid">




<%= yield %>

</div>

<script>
  $( function () {
    filepicker.setKey("<%= ENV['FILEPICKER_KEY'] %>");
  })
</script>
</body>
</html>
