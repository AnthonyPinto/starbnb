<div class="search-sidebar">
<h1>StaticPages#search</h1>
<form>
  <div class="form-group">
    <input type='text'
      name="space[checkIn]"
      class="form-control datetimepicker"
      placeholder="Check In" 
      id='datetimepickerStart'/>
    <input 
      name="space[checkOut]"
      type='text' 
      class="form-control datetimepicker"
      placeholder="Check Out"  
      id='datetimepickerEnd'/>
  </div>
  <div class="checkbox" name="space[style]">
    <label>
      <input class="checkbox-choice" type="checkbox" value="planet"> Planet
    </label>
    <label>
      <input class="checkbox-choice" type="checkbox" value="moon"> Moon
    </label>
    <label>
      <input class="checkbox-choice" type="checkbox" value="station"> Station
    </label>
  </div>
  <div class="form-group">
    <label for="price-slider">Price Range</label>
    <div id="price-slider" name="space[priceRange]"></div>
    <input id="price-lower"></input>
    <input id="price-upper"></input>
  </div>
</form>
<div class="results-wrapper">
</div>