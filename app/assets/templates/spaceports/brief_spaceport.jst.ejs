
<div class="brief-img-wrapper col-xs-12">
  <div class="brief-price-frame">
    <span class="brief-dollar price-color"><strong>$</strong></span>
    <h3 class="brief-price price-color"><%= spaceport.escape('price') %></h3>
  </div>
  <div id="dummy-x"></div>
  <div id="element-x">
    <img src="<%= spaceport.photos().at(0).escape('url') %>" class="brief-img">
  </div>
</div>


<div class="brief-title-wrapper col-xs-12">
    <h4 class="brief-title"><%= spaceport.escape('name') %></h4>
    <div class="brief-user-img-frame">
      <img 
        src="<%= spaceport.userPhoto().escape('url') %>" 
        class="brief-user-img">
    </div>
</div>
<div class="brief-text-wrapper col-xs-12">
    <h5 class="brief-text">
      <%= spaceport.styleStr() + " â€¢ " + spaceport.escape('address') %>
    </h5>
</div>



